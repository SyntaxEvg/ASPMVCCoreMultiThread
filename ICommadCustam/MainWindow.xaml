<Window x:Class="ICommadCustam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICommadCustam"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="TextMessage" Text="TextMessage"></TextBlock>
            <Button Content="Command" Command="{Binding Click}" CommandParameter ="{Binding Text, ElementName=TextMessage}" />
        </StackPanel>
    </Grid>
</Window>
